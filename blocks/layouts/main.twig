<!DOCTYPE HTML>
<html>
<head>
	<title>{% block title %}{{ config.site_name }}{% endblock %}</title>
	<meta http-equiv="content-type" content="text/html; charset=utf-8" />
	<meta name="description" content="" />
	<meta name="keywords" content="" />
	<!--[if lte IE 8]><script src="{{ asset('css/ie/html5shiv.js') }}"></script><![endif]-->
	<script src="{{ asset('js/jquery.min.js') }}"></script>
	<script src="{{ asset('js/jquery.dropotron.min.js') }}"></script>
	<script src="{{ asset('js/skel.min.js') }}"></script>
	<script src="{{ asset('js/skel-layers.min.js') }}"></script>
	<script src="{{ asset('js/init.js') }}"></script>
	<noscript>
		<link rel="stylesheet" href="{{ asset('css/skel.css') }}" />
		<link rel="stylesheet" href="{{ asset('css/style.css') }}" />
		<link rel="stylesheet" href="{{ asset('css/style-desktop.css') }}" />
	</noscript>
	<link rel="stylesheet" href="{{ asset(__('localized_style')) }}" />
	<!--[if lte IE 8]><link rel="stylesheet" href="{{ asset('css/ie/v8.css') }}" /><![endif]-->
	{% block head %}{% endblock %}
</head>
<body class="{% block body_class %}{% endblock %}">

<!-- Header -->
<div id="header-wrapper">
	<header id="header" class="container">

		<!-- Logo -->
		{% block logo %}
			{% include '@blocks/modules/logo.twig' %}
		{% endblock nav %}


		{% block nav %}
			{% include '@blocks/modules/nav.twig' %}
		{% endblock nav %}

		{% block featuredcontent %}
			{% include '@blocks/modules/featuredcontent.twig' %}
		{% endblock featuredcontent %}
		<!-- Nav -->
		
	</header>
</div>


<div>
	{% block main %}{% endblock %}
</div>


<!-- Footer -->
{% block footer %}
	{% include '@blocks/modules/footer.twig' %}
{% endblock footer %}


</body>
</html>